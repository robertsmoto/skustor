type Collection {
  id: ID!
  document: JSON!
  parentId: ID
  imageIds: ID
  itemIds: ID
}

type Query {
  collections: [Collection!]!
}

input NewCollection {
  id: ID!
  document: JSON!
}

type Mutation {
  addCollection(input: NewCollection!): Collection!
  addCollections(input: [NewCollection!]): [Collection!]
}

